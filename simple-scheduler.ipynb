    "from apscheduler.schedulers.blocking import BlockingScheduler\n",
    "import datetime\n",
    "\n",
    "def some_job():\n",
    "    print(\"Hello World\")\n",
    "    print(datetime.datetime.now())\n",
    "\n",
    "scheduler = BlockingScheduler()\n",
    "scheduler.add_job(some_job, 'interval', seconds=5)\n",
    "scheduler.start()"
